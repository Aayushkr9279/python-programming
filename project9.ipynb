    "import speech_recognition as sr\n",
    "mic = sr.Recognizer()\n",
    "\n",
    "with sr.Microphone() as source:\n",
    "    print(\"Start Speaking...\")\n",
    "    voice = mic.listen(source)\n",
    "    text = mic.recognize_google(voice)\n",
    "    print (\"you said this : \", text)"
